{
	"name": "keepsidian",
	"version": "1.1.1",
	"description": "Sync Google Keep notes to Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "npm run lint:ts && tsc --project tsconfig.json --noEmit --skipLibCheck && node esbuild.config.mjs production",
		"prebuild": "npm run barrels",
		"barrels": "barrelsby --config barrelsby.json",
		"release": "node scripts/release.mjs",
		"version": "node scripts/version-bump.mjs && git add manifest.json versions.json",
		"lint": "npm run lint:ts && npm run lint:md",
		"lint:ts": "ESLINT_USE_FLAT_CONFIG=true npx eslint -c eslint.config.mjs src",
		"lint:ts:fix": "ESLINT_USE_FLAT_CONFIG=true npx eslint -c eslint.config.mjs src --fix",
		"lint:md": "markdownlint \"**/*.md\" --ignore node_modules -c markdownlint.json",
		"lint:md:fix": "markdownlint \"**/*.md\" --ignore node_modules -c markdownlint.json --fix",
		"lint:fix": "npm run lint:ts:fix && npm run lint:md:fix",
		"preversion": "npm run lint",
		"test": "npx jest",
		"coverage": "npx jest --coverage"
	},
	"keywords": [
		"obsidian",
		"google keep",
		"sync",
		"plugin"
	],
	"author": "lc0rp",
	"license": "MIT",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.6.2",
		"@types/electron": "^1.4.38",
		"@types/jest": "^29.5.14",
		"@types/node": "^16.18.104",
		"@typescript-eslint/eslint-plugin": "^8.44.1",
		"@typescript-eslint/parser": "^8.44.1",
		"barrelsby": "^2.8.1",
		"builtin-modules": "3.3.0",
		"electron": "^32.1.2",
		"esbuild": "0.17.3",
		"eslint": "^9.36.0",
		"eslint-plugin-obsidianmd": "^0.1.4",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"obsidian": "^1.6.6",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@jest/globals": "^29.7.0",
		"automatonic": "^0.1.1",
		"playwright": "^1.45.3",
		"puppeteer-chromium-resolver": "^22.0.0",
		"zod": "^4.1.8"
	}
}
