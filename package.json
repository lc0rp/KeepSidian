{
  "name": "keepsidian",
  "version": "2.0.3-alpha.3",
  "description": "Sync Google Keep notes to Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "npm run lint:ts && tsc --project tsconfig.json --noEmit --skipLibCheck && node esbuild.config.mjs production",
    "prebuild": "npm run barrels",
    "barrels": "barrelsby --config barrelsby.json",
    "release": "node scripts/release.mjs",
    "version": "node scripts/version-bump.mjs && git add manifest.json versions.json",
    "format": "prettier \"apps/**/*.{ts,tsx,md}\" --write",
    "format-docs": "prettier \"docs/**/*.{ts,tsx,md}\" --write",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:links && npm run lint:ia",
    "lint:ts": "ESLINT_USE_FLAT_CONFIG=true npx eslint -c eslint.config.mjs src",
    "lint:ts:fix": "ESLINT_USE_FLAT_CONFIG=true npx eslint -c eslint.config.mjs src --fix",
    "lint:md": "markdownlint-cli2 '**/*.md'",
    "lint:md:fix": "markdownlint-cli2 --fix '**/*.md'",
    "lint:links": "node scripts/link-check.mjs",
    "lint:links:fix": "node scripts/link-check.mjs --fix",
    "lint:ia": "node scripts/ia-validate.mjs",
    "lint:fix": "npm run lint:ts:fix && npm run lint:md:fix",
    "preversion": "npm run lint",
    "test": "npx jest",
    "coverage": "npx jest --coverage",
    "bmad:refresh": "bmad-method install -f -i codex",
    "bmad:list": "bmad-method list:agents",
    "bmad:validate": "bmad-method validate",
    "wdio": "wdio run ./wdio.conf.mts",
    "wdio:mobile": "wdio run ./wdio.mobile.conf.mts",
    "e2e:desktop": "npm run build && npm run wdio",
    "e2e:android": "npm run build && npm run wdio:mobile",
    "appium:android": "ANDROID_SDK_ROOT=\"$HOME/Library/Android/sdk\" ANDROID_HOME=\"$HOME/Library/Android/sdk\" PATH=\"$HOME/Library/Android/sdk/platform-tools:$HOME/Library/Android/sdk/emulator:$HOME/Library/Android/sdk/cmdline-tools/latest/bin:$PATH\" npx appium server --address 127.0.0.1 --port 4723 --allow-insecure \"*:chromedriver_autodownload,*:adb_shell\""
  },
  "keywords": [
    "obsidian",
    "google keep",
    "sync",
    "plugin"
  ],
  "author": "lc0rp",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.2",
    "@types/electron": "^1.4.38",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^16.18.126",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@wdio/appium-service": "^9.21.0",
    "@wdio/cli": "^9.21.1",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.21.0",
    "@wdio/mocha-framework": "^9.21.0",
    "@wdio/spec-reporter": "^9.20.0",
    "appium": "^3.1.2",
    "appium-uiautomator2-driver": "^6.7.2",
    "barrelsby": "^2.8.1",
    "builtin-modules": "3.3.0",
    "electron": "^32.1.2",
    "esbuild": "^0.17.3",
    "eslint": "^9.39.2",
    "eslint-plugin-obsidianmd": "^0.1.4",
    "expect-webdriverio": "^5.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdownlint-cli2": "^0.20.0",
    "mocha": "^11.7.5",
    "obsidian": "^1.6.6",
    "prettier": "^3.7.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "2.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "wdio-obsidian-reporter": "^2.1.6",
    "wdio-obsidian-service": "^2.1.6"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "automatonic": "^0.1.1",
    "playwright": "^1.45.3",
    "puppeteer-chromium-resolver": "^22.0.0",
    "zod": "^4.1.8"
  }
}
